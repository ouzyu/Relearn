<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h2><%= @range %> search for "<%= @word %>"</h2>
      <% if @range == "User" %>
        <%= render 'users/index', users: @users %>
      <% elsif @range == "Book" %>
        <%= render 'books/index', books: @books %>
      <% end %>
    </div>
  </div>
</div>